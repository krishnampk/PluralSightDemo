<Window x:Class="PluralSightPrismDemo.Shell"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://www.codeplex.com/prism"
        xmlns:infr="clr-namespace:PluralsightPrismDemo.Infrastructure;assembly=PluralsightPrismDemo.Infrastructure"
        Title="Shell">
    <DockPanel LastChildFill="True">
        
        <ContentControl DockPanel.Dock="Top" prism:RegionManager.RegionName= "{x:Static infr:RegionNames.ContentRegion}"></ContentControl>
    </DockPanel>
</Window>
